#################################################################

###  http://schema.ufokn.org/core/v1/depth
:depth rdf:type rdfs:Datatype .


###  http://schema.ufokn.org/core/v1/elevation
:elevation rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://schema.ufokn.org/core/v1/associatedDataSource
:associatedDataSource rdf:type owl:ObjectProperty ;
                      rdfs:domain owl:Thing ;
                      rdfs:range :DataSource ;
                      rdfs:comment "A representation of a data source that contains things like model outputs, spatial features, risk points, or similar."@en ;
                      rdfs:label "associatedDataSource"@en .


###  http://schema.ufokn.org/core/v1/comid
:comid rdf:type owl:ObjectProperty .


###  http://schema.ufokn.org/core/v1/hasRelativeRiskPointLocation
:hasRelativeRiskPointLocation rdf:type owl:ObjectProperty ;
                              owl:inverseOf :locationForRiskPoint ;
                              rdfs:domain :RiskPointFeature ;
                              rdfs:range :RelativeRiskPointLocation ;
                              rdfs:comment "Relationship between a RiskPointFeature and its Localication Relative to the Spatial Model used by a Specific Modeling or Observation System. The relative location allows associating different elevations with a RiskPoint based on different spatial interpolation methods."@en ;
                              rdfs:label "hasRelativeRiskPointLocation"@en .


###  http://schema.ufokn.org/core/v1/hasRiskPointOfInterest
:hasRiskPointOfInterest rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf sosa:hasFeatureOfInterest ;
                        rdfs:range [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasRiskPointOfInterest ;
                                     owl:someValuesFrom :RiskPointFeature
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasRiskPointOfInterest ;
                                     owl:allValuesFrom :RiskPointFeature
                                   ] .


###  http://schema.ufokn.org/core/v1/identifiedBy
:identifiedBy rdf:type owl:ObjectProperty .


###  http://schema.ufokn.org/core/v1/locationForRiskPoint
:locationForRiskPoint rdf:type owl:ObjectProperty .


###  http://www.opengis.net/spec/geosparql/1.0/hasGeometry
geo:hasGeometry rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/sosa/hasFeatureOfInterest
sosa:hasFeatureOfInterest rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://schema.ufokn.org/core/v1/criticalDepth
:criticalDepth rdf:type owl:DatatypeProperty ;
               rdfs:domain :RiskPointFeature ;
               rdfs:range :depth .


###  http://schema.ufokn.org/core/v1/heightAboveNearestDrainage
:heightAboveNearestDrainage rdf:type owl:DatatypeProperty ;
                            rdfs:domain :RelativeRiskPointLocation ;
                            rdfs:range :elevation .


#################################################################
#    Classes
#################################################################

###  http://schema.ufokn.org/core/v1/DataSource
:DataSource rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ;
            owl:disjointWith geo:Feature ,
                             geo:Geometry .


###  http://schema.ufokn.org/core/v1/FeatureAtRisk
:FeatureAtRisk rdf:type owl:Class ;
               rdfs:subClassOf geo:Feature ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :identifiedBy ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Identifier
                               ] .


###  http://schema.ufokn.org/core/v1/Forecast
:Forecast rdf:type owl:Class ;
          owl:disjointWith sosa:Observation .


###  http://schema.ufokn.org/core/v1/Identifier
:Identifier rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :identifiedBy ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] .


###  http://schema.ufokn.org/core/v1/RelativeRiskPointLocation
:RelativeRiskPointLocation rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty :locationForRiskPoint ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass :RiskPointFeature
                                           ] .


###  http://schema.ufokn.org/core/v1/RiskPointFeature
:RiskPointFeature rdf:type owl:Class ;
                  rdfs:subClassOf geo:Feature ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty geo:hasGeometry ;
                                    owl:allValuesFrom geo:Point
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :associatedDataSource ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :DataSource
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :comid ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Identifier
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :identifiedBy ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :Identifier
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty geo:hasGeometry ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass geo:Point
                                  ] .


###  http://schema.ufokn.org/core/v1/RiskPointForecast
:RiskPointForecast rdf:type owl:Class ;
                   rdfs:subClassOf :Forecast ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty sosa:hasFeatureOfInterest ;
                                     owl:someValuesFrom :RiskPointFeature
                                   ] .


###  http://schema.ufokn.org/core/v1/RiskPointObservation
:RiskPointObservation rdf:type owl:Class ;
                      rdfs:subClassOf sosa:Observation ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty sosa:hasFeatureOfInterest ;
                                        owl:someValuesFrom :RiskPointFeature
                                      ] .


###  http://www.opengis.net/spec/geosparql/1.0/Feature
geo:Feature rdf:type owl:Class .


###  http://www.opengis.net/spec/geosparql/1.0/Geometry
geo:Geometry rdf:type owl:Class .


###  http://www.opengis.net/spec/geosparql/1.0/Point
geo:Point rdf:type owl:Class .


###  http://www.w3.org/ns/sosa/Observation
sosa:Observation rdf:type owl:Class .


